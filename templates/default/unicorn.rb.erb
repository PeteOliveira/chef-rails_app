working_directory "<%= @app['@app['deploy_to']'] %>/current"
pid "<%= @app['deploy_to'] %>/shared/pids/unicorn.pid"
stderr_path "<%= @app['deploy_to'] %>/shared/log/unicorn.log"
stdout_path "<%= @app['deploy_to'] %>/shared/log/unicorn.log"

listen "<%= @app['deploy_to'] %>/shared/unicorn.sock"
worker_processes <%= @app['worker_count'] %>
timeout 30